{
    "name": "darkperis/wcms-polls",
    "description": "A Laravel package to create polls on Barb CMS",
    "keywords": ["polls", "votes", "laravel", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Periklis Diamantidis",
            "email": "p.diamantidis@darkpony.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
      "psr-4": {
        "Darkpony\\WCMSPolls\\": "src/"
      }
    },
    "require": {
      "illuminate/support": "~5|~6|~7|~8|^9|^10.0|^11.0"
    },
    "require-dev": {
      "fzaninotto/faker": "~1.4",
      "phpunit/phpunit": "~4.0"
    },
    "extra": {
      "laravel": {
        "providers": [
          "Darkpony\\WCMSPolls\\WCMSPollsServiceProvider"
        ],
        "aliases": {
          "PollWriter": "Darkpony\\WCMSPolls\\PollWriterFacade"
        }
      }
    }
}
